    object tsDescription: TTabSheet
      Caption = 'Descripcion'
      ClientHeight = 352
      ClientWidth = 441
      object Memo2: TMemo
        Left = 0
        Height = 352
        Top = 0
        Width = 441
        Align = alClient
        Alignment = taCenter
        Font.Height = -11
        Font.Name = 'Verdana'
        Lines.Strings = (
          ''
          '--------------------'
          'FPG Editor r'
          '--------------------'
          ''
          'Es un visualizador/editor de ficheros de imágenes (FPG) para '
          'aplicaciones multimedia realizados en DIV2, FENIX, CDIV y BennuGD.'
          ''
          'Este programa trata de dar un soporte para la edición de '
          'ficheros FPG de una forma sencilla y eficiente.'
          ''
          'Este programa es de software libre, usted puede distribuirlo '
          'y/o '
          'modificarlo bajo los terminos de licencia: '
          'GNU (GENERAL PUBLIC LICENSE)'
          ''
          'Este programa se distribuye para que todos lo puedan utilizar, '
          'pero NO TIENE NINGUNA GARANTIA. Por lo que el autor no se '
          'hace responsable de cualquier mal funcionamiento o daño que '
          'puediese ocasionar.'
        )
        OnChange = Memo2Change
        ParentFont = False
        ReadOnly = True
        ScrollBars = ssVertical
        TabOrder = 0
      end
    end
    object tsCharacteristic: TTabSheet
      Caption = 'Caracteristicas'
      ClientHeight = 352
      ClientWidth = 441
      ImageIndex = 1
      object Memo1: TMemo
        Left = 0
        Height = 352
        Top = 0
        Width = 441
        Align = alClient
        Font.Height = -11
        Font.Name = 'Verdana'
        Lines.Strings = (
          'Tiene la capacidad de abrir, cargar, convertir, visualizar, modificar, '
          'extraer todo lo que contienen los ficheros FPG y FNT, '
          'comprimidos y sin comprimir de entornos de programación DIV-like.'
          ''
          'Soporta:'
          ' + FPG y FNT de 1 bit para Fenix y BennuGD '
          ' + FPG y FNT de 8 bits para (DIV2,  CDIV, Fenix, BennuGD y Gemix)'
          ' + FPG y FNT de 16 bits para Fenix y BennuGD'
          ' + FPG y FNTde 16 bits para CDIV'
          ' + FPG y FNT de 24 bits. '
          ' + FPG y FNT de 32 bits para BennuGD'
          ' + Conversión de un formato a otro'
          ' + Sistema de multi-lenguaje auto-adaptable'
          ' + Manejo del portapapeles para imágenes '
          '    (copiar, pegar y vaciar portapapeles)'
          ' + Capacidad de exportar todo lo que contine un FPG '
          '    ( Imágenes, paletas y puntos de control )'
          ' + "Drag and Drop" arrastrar y soltar, tanto para añadir imágenes al '
          'FPG o FNT'
          '    como para exportar las imágenes del FPG o FNT'
          ' + Selección de puntos de control de forma gráfica, con la posibilidad '
          'de '
          '    cargar desde un fichero de texto .CPT'
          ' + Sistema de animación para las imágenes de los ficheros FPG'
          ' + Filtrado de imágenes personalizado'
          ' + Compresor para los FPG de Fenix/BennuGD'
          ' + Mini Editor de ficheros de código PRG'
          ' + Generador de FNTs.'
          ' + Mini Editor de Imágenes con soporte de Gamas de Colores en 8 bits'
          ''
          'Formatos de imágenes:'
          ' PNG, MNG, JNG, JPG, JIF, GIF, BMP, WMF,  EMF, DIB, RLE, TGA, '
          ' PCX, ICO, TIF, TIFF, FAX, BW, RGB, RGBA, SGI, CEL, PIC, VST, ICB, '
          ' VDA, WIN, PCC, SCR, PCD, PPM, PGM, CUT, PAL, RLA, RPF, PSD, '
          ' PDD, PSP.'
          ''
          'Formatos de paletas:'
          ' PAL (DIV2, MICROSOFT,  PAINT SHOP PRO 4)'
          ''
          'Importa paletas de ficheros de 8 bits de: '
          ' BMP, PCX, FPG, MAP y FNT.'
          ''
          'Carga y extrae puntos de control a:'
          ' CPT.'
        )
        ParentFont = False
        ReadOnly = True
        ScrollBars = ssVertical
        TabOrder = 0
      end
    end
